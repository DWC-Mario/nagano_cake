<h3>商品新規登録</h3>
<table>
<%= form_with model: @item do |f| %>
  <tr>
    <th>商品画像</th> <td><%= f.file_field :image, accept: "image/*" %></td>
  </tr>
  <tr>
    <th>商品名</th> <td><%= f.text_field :item_name %></td>
  </tr>
  <tr>
    <th>商品説明</th> <td> <%= f.text_area :item_explanation %></td>
  </tr>
  <tr>
    <th>ジャンル</th> <td></td>
  </tr>
  <tr>
    <th>税抜き価格</th> <td></td>
  </tr>
  <tr>
    <th>販売ステータス</th> <td></td>
  </tr>
  <tr>
    <td><%= f.submit '投稿' %></td>
  </tr>
  <% end %>
  </table>
