<div class="container">
  <div class="m-3"><h5 class="d-inline highlight">管理者ログイン</h5></div>
    <div class="m-3">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <table class="table table-borderless">
        <!--bootstrapが当たらない-->
        <tr>
        <tr>
          <td class="col-2 py-1"><%= f.label :email, "メールアドレス"%></td>
          <td class="col-5"><%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control custom-input" %></td>
        </tr>
        <tr>
          <td class="py-1"><%= f.label :password, "パスワード" %></td>
          <td class=""><%= f.password_field :password, autocomplete: "current-password", class: "form-control custom-input" %></td>
        </tr>
        <tr>
          <td class="py-1"></td>
          <td class=""><%= f.submit "ログイン", class: "btn custom-button form-control", style: "width: 30%" %></td>
        </tr>
      </table>
    <% end %>
    </div>
    
      
</div>